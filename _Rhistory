library(prob)
library(prob)
p <- rep(1/16, times=8)                                  #-- probability p
A = urnsamples(1:2, size=3, replace=TRUE, ordered=TRUE)  #-- outcomes of events A
S  = probspace(A, probs=p)       #-- probability space S(events A, probability p)
A2 = subset(A, X1+X2>=3);  A2    #== event A2 is subset of A
S2 = subset(S, X1+X2>=3);  S2    #== subspace S2 of probability space S
A3 = subset(A, X1+X3>=3);  A3    #== event A2 is subset of A
S3 = subset(S, X1+X3>=3);  S2    #-- subspace S3
A2and3 = intersect(A2, A3);  A2and3
S2and3 = subset(S, (X1+X2>=3) & (X1+X3>=3));  S2and3
Prob(S, X1+X2>=3, given = X1+X3>=3)  #-- conditional probability P(X1+X2>=3 | X1+X3>=3)
